-- Create the NLCD designation CSV files
drop schema nlcd cascade;
create schema nlcd;

create table class (
 class integer primary key,
 major text,
 name text,
 description text
);
\copy class from nlcd_2006_class.csv with csv header

create table crosswalk (
 dau_id integer,
 dau varchar(8),
 grid_id integer,
 dwr_id varchar(7),
 nlcd_class integer,
 nlcd_name text,
 count integer
);
\copy crosswalk from daul.hist with csv

create table crosswalk2 (
 dau varchar(8),
 dwr_id varchar(7),
 nlcd_class integer,
 count integer
);
\copy crosswalk2 (null,dau,null,dwr_id,nlcd,null,count) from daul.hist with csv
